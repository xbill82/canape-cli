{
  "name": "canape-cli",
  "description": "A CLI to automate tasks at Le CanapÃ© dans l'Arbre",
  "version": "0.0.0",
  "author": "Luca Marchesini",
  "bin": {
    "canape": "./bin/run.js"
  },
  "bugs": "https://github.com/xbill82/canape-cli/issues",
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@types/handlebars-helpers": "^0.5.6",
    "@types/lodash": "^4.17.7",
    "@types/rc": "^1.2.4",
    "dayjs": "^1.11.12",
    "handlebars": "^4.7.8",
    "handlebars-helpers": "^0.10.0",
    "lodash": "^4.17.21",
    "pkg-dir": "^8.0.0",
    "puppeteer": "^23.0.2",
    "rc": "^1.2.8",
    "throttled-queue": "https://github.com/r1ddev/throttled-queue-esm"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/xbill82/canape-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "canape",
    "dirname": "canape",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "contract": {
        "description": "Generate a contract for a specific deal"
      }
    }
  },
  "repository": "xbill82/canape-cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
